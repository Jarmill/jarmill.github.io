  <span class="na">slides</span> <span class="p">=</span> <span class="s">{Thesis_Presentation_Merged.pdf}</span><span class="p">,</span>
  <span class="na">poster</span> <span class="p">=</span> <span class="s">{Safety_Poster__PhD_Expo.pdf}</span><span class="p">,</span>
  <span class="na">tag</span> <span class="p">=</span> <span class="s">{peakddc}</span>
<span class="p">}</span></code></pre></figure>
          </div>
        </div>
