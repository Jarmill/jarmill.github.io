  <span class="na">html</span> <span class="p">=</span> <span class="s">{https://ieeexplore.ieee.org/abstract/document/9971732}</span><span class="p">,</span>
  <span class="na">code</span> <span class="p">=</span> <span class="s">{https://github.com/Jarmill/lpv_qmi}</span><span class="p">,</span>
