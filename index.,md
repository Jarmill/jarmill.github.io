  <span class="na">journal</span> <span class="p">=</span> <span class="s">{IEEE Transactions on Automatic Control}</span><span class="p">,</span>
  <span class="na">abbr</span> <span class="p">=</span> <span class="s">{TAC}</span><span class="p">,</span>
