@preprint{miller2023hybrid,
      title={{Peak Estimation of Hybrid Systems with Convex Optimization}}, 
      author={Jared Miller and Mario Sznaier},
      year={2023},
      eprint={2303.11490},
      archivePrefix={arXiv},
	  url={https://arxiv.org/abs/2303.11490},
