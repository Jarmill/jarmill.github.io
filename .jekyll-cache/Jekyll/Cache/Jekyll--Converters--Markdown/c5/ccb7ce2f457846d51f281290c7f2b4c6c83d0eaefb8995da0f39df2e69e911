  <span class="na">html</span> <span class="p">=</span> <span class="s">{https://www.sciencedirect.com/science/article/pii/S2405896320316645}</span><span class="p">,</span>
  <span class="na">slides</span> <span class="p">=</span> <span class="s">{IFAC_decomposed.pdf}</span><span class="p">,</span>
